@{
    ViewBag.Title = "EditStaff";
    var id = ViewBag.id;
}
<h2>Edit</h2>

<div id="divStaffEdit">
    <a href="~/Staff/Index?id=@ViewBag.id">Back</a><br /><br />
    Email : <input type="text" data-bind="value: $data.first" /><br />
    <br />
    Password : <input type="password" data-bind="value: $data.last" /><br />
    <br />
    <input type="button" value="Update" data-bind="value: 'Update', click: $data.update" style="margin-left:80px" />
    <br /><br />
    <div id="divMessage"></div>
</div>